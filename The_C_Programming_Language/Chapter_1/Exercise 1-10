#include <stdio.h>

main(){

    int c;


    while((c = getchar()) != EOF){
        if(c == '\t'){
            printf("\\t");
        }else if(c == '\b'){
            printf("\\b\n");
        }else if(c == '\\'){
            printf("\\\\\n");
        }else if(c != '\t' && c != '\b' && c != '\\'){
            putchar(c);
        }
    }
    
    return 0;
}

/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

    while((c = getchar()) != EOF){
        switch(c){
            case '\t':{
                printf("\\t");
                break;
            }
            case '\b':{
                printf("\\b");
                break;
            }
            case '\\':{
                printf("\\\\");
                break;
            }
            default: {
                putchar(c);
            }
        }
    }

    return 0;
}
