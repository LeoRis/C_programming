#include <stdio.h>

int main(){
    // Printing Fahrenheit to Celsius degrees table.
    int upper = 300;

    float fahr;

    printf("Fahrenheit \t Celsius \n");
    printf("------------------------\n");

    for(fahr = 0; fahr < upper; fahr += 20){
        printf("%.02f \t\t %.02f \n", fahr, fahr - 32 * (5.0 / 9.0));
    }
    
    return 0;
}    
